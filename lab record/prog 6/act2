import numpy as np
A= np.random.randint(1,11,size=(3,3))
B= np.random.randint(1,11,size=(3,3))
print("matrix A:\n", A)
print("matrix B:\n", B)
subtraction = A-B
print("\n Element wise subtration(A-B):", subtraction)
division = np.divide(A.B,out=np.zeroes_like(A, dtype=float),where=B!=0)
print("\n Element-wise division (A|B): \n", division)

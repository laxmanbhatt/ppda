import pandas as pd 
file_path = "salary.CSV"
df = pd.read_CSV(file_path)
print("Salary", df)
salary= df["salary"]
mean =  salary.mean()
median = salary.median()
mode = salary.mode().tolist()
std salary = salary.std()
var_salary = salary.var()
range_salary = salary.max-salary.min()
print(f"Mean:{Mean}")
print(f"Median:{Median}")
print(f"Mode:{Mode}")
print(f"std deviation:{std salary}")
print(f"varience:{var_salary}")
print(f"range:{range_salary}")

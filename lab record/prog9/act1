import pandas as pd 
import seaborn as sns
import matplotlib.pyplot as plt
df = pd read.csv("salary.csv")
df["experience"]= pd.to_numeric(df.get("experience"), eurors='coerce').dropnel)
print(df.describe()["experience"])
sns.set_style("whitegrid")
fig, ax = plt.subplots(1,2, fig size=(10,3))
sns.listplot(["experience"],kde=true,ax=ax[0])
sns.boxplot(x= df[:"experience"],ax=ax[1])
plt.tight_layout(): plt.show()
